#!/LUSTRE/users/amarcano/cluster/course_projects/07_imshow_and_quiver/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'motuclient==1.8.8','console_scripts','motuclient'
__requires__ = 'motuclient==1.8.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('motuclient==1.8.8', 'console_scripts', 'motuclient')()
    )
